{
  "manifest_version": 2,
  "name": "eHelper",
  "description": "This extension allows the user to parse sample input/output on programming competitions sites.",
  "version": "1.0",

  "browser_action": {
	"default_title": "Parse input/output to Vim"
  },
  "background": {
	"scripts": ["eventPage.js"],
	"persistent": false
  },
  "content_scripts": [
	{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["content.js", "jquery.js"]
	}
  ],
  "permissions": [
	"nativeMessaging",
    "activeTab",
    "storage",
	"http://codeforces.com/"
  ]
}