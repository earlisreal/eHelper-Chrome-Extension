{
  "manifest_version": 2,
  "name": "eHelper",
  "description": "This extension allows the user to parse sample input/output on programming competitions sites.",
  "version": "1.0",

  "page_action": {
	"default_title": "Parse input/output to Vim"
  },
  "background": {
	"scripts": ["eventPage.js", "jquery.js"],
	"persistent": false
  },
  "content_scripts": [
	{
		"matches": [ "https://*.contest.yandex.ru/*", "https://*.contest.yandex.com/*", "https://*.contest2.yandex.ru/*", "https://*.contest2.yandex.com/*", "http://codeforces.ru/*", "http://codeforces.com/*", "https://hackerrank.com/*", "https://www.hackerrank.com/*", "https://www.facebook.com/hackercup/*", "http://usaco.org/*", "http://www.usaco.org/*", "https://code.google.com/codejam/contest/*", "http://contest.bayan.ir/*", "https://*.kattis.com/problems/*", "https://*.kattis.com/contests/*", "http://www.codechef.com/*", "https://www.codechef.com/*", "https://www.hackerearth.com/*", "http://*.contest.atcoder.jp/*", "https://csacademy.com/*" ],
		"js": ["content.js", "jquery.js"],
        "run_at": "document_idle"
	}
  ],
  "permissions": [
	"nativeMessaging",
    "tabs",
    "activeTab",
    "storage"
  ]
}
